{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "target": "ESnext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "ES2020"
    ],
    "outDir": "dist",
    "sourceMap": true,
    "module": "es2020",
    "moduleResolution": "bundler",
    "types": [
      "cypress",
      "jest"
    ],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "cypress/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "test/**/*"
  ]
}